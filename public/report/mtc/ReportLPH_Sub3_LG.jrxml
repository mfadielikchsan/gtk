	<queryString>
		<![CDATA[SELECT KD_PLANT, CASE WHEN KD_PLANT IN ('A', 'B') THEN 'KIM-1'||KD_PLANT ELSE 'IGP-'||KD_PLANT END AS NM_PLANT, COALESCE(SUM(JM.JML_PLAN),0) J_PLAN, COALESCE(SUM(JM.JML_ACT),0) J_ACT 
FROM (
	SELECT KD_PLANT, COUNT(NO_PMS) JML_PLAN, 0 JML_ACT
	FROM MTCT_PMS
	WHERE thn_pms||bln_pms = TO_CHAR($P{tgl1}::date,'YYYYMM') 
	AND thn_pms||bln_pms||lpad(tgl_pms::text,2,'0') < to_char(now(),'yyyymmdd') 
	AND KD_PLANT = $P{lok_pt}
	AND (KD_LINE = $P{kd_line} OR $P{kd_line} = '')
	AND (KD_MESIN = $P{kd_mesin} OR $P{kd_mesin} = '')
	AND ST_CEK = 'T'
	GROUP BY KD_PLANT
	UNION ALL
	SELECT KD_PLANT, 0 JML_PLAN, COUNT(NO_PMS) JML_ACT
	FROM MTCT_PMS
	WHERE thn_pms||bln_pms = TO_CHAR($P{tgl1}::date,'YYYYMM') 
	AND thn_pms||bln_pms||lpad(tgl_pms::text,2,'0') < to_char(now(),'yyyymmdd') 
	AND KD_PLANT = $P{lok_pt}
	AND (KD_LINE = $P{kd_line} OR $P{kd_line} = '')
	AND (KD_MESIN = $P{kd_mesin} OR $P{kd_mesin} = '')
	AND ST_CEK = 'T'
	AND TGL_TARIK IS NOT NULL
	GROUP BY KD_PLANT
) JM
GROUP BY JM.KD_PLANT
ORDER BY 2]]>
	</queryString>
