	<queryString>
		<![CDATA[select kd_line||' - '||fnm_linex(kd_line) line, count(no_dm) jml_dm
from mtct_dft_mslh
where to_char(tgl_dm,'yyyymmdd') >= to_char($P{tgl1}::date,'yyyymmdd') 
and to_char(tgl_dm,'yyyymmdd') <= to_char($P{tgl2}::date,'yyyymmdd')
and kd_plant = $P{lok_pt}
and (kd_line = $P{kd_line} or $P{kd_line} = '')
and (kd_mesin = $P{kd_mesin} or $P{kd_mesin} = '')
and exists (select 1 from mtcm_npk where mtcm_npk.npk = $P{npk} and mtcm_npk.kd_plant = mtct_dft_mslh.kd_plant)
group by kd_line
order by 1]]>
	</queryString>
