	<queryString>
		<![CDATA[SELECT NO_WO, TGL_WO, KD_STATUS, PT, LOK_PT, KD_LINE, FNM_LINEX(KD_LINE) NM_LINE,  KD_PROS, KD_MESIN, KD_PROBLEM, URAIAN_PROB, EST_JAMSTART, EST_JAMEND, EST_DURASI, DTCREA, DTMODI, TYPE_MTC, URAIAN_PENYEBAB, USULAN_PERBAIKAN, CATATAN, TYPE_KERJA TYPE_INFO_KERJA, NO_DM, TGL_PLAN, LINE_STOP, ST_CHECK, CREABY,  MODIBY, FREKUENSI, LANGKAH_KERJA, SHIFT, ST_UTILITY, BIAYA_JAM_WO, RANGKING, PRSN_BREAKDOWN, TOTAL_LINESTOP, PRINT_BIGPROBLEM, SPAREPART, NO_LS_IGP4, KD_PLANT, NM_PELAKSANA, KD_SITE, INFO_KERJA, ST_CLOSE, LOK_PICT, NO_PMS, APR_PIC_TGL, APR_PIC_NPK, APR_FM_TGL, APR_FM_NPK, APR_SH_TGL, APR_SH_NPK, RJT_TGL, RJT_NPK, RJT_KET, RJT_ST, ST_MAIN_ITEM, NO_LHP, LS_MULAI, NO_IC, NO_LS, PREVENT_DD, PREVENT_STATUS, QTY_OLI 
FROM TMTCWO1 
WHERE (TO_CHAR(TGL_WO,'YYYYMMDD') >= $P{tgl1} 
AND TO_CHAR(TGL_WO,'YYYYMMDD') <= $P{tgl2}) 
AND (SHIFT = $P{shift} OR $P{shift} = '') 
AND (LOK_PT = $P{lok_pt} OR $P{lok_pt} = '') 
AND (KD_LINE = $P{kd_line} OR $P{kd_line} = '') 
AND (KD_MESIN = $P{kd_mesin} OR $P{kd_mesin} = '') 
AND (COALESCE(INFO_KERJA,'-') = $P{st_pms} OR $P{st_pms} = '') 
AND COALESCE(LINE_STOP,0) >= COALESCE($P{line_stop},0)
AND EXISTS (SELECT 1 FROM MTCM_NPK WHERE MTCM_NPK.NPK = $P{npk} AND MTCM_NPK.KD_PLANT = TMTCWO1.LOK_PT)]]>
	</queryString>
