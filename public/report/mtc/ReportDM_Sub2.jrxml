	<queryString>
		<![CDATA[select m1.tgl_dm, m1.kd_mesin||' - '||fnm_mesinx(m1.kd_mesin) as mesin, m1.kd_line||' - '||fnm_linex(m1.kd_line) line, m1.ket_prob, m1.ket_cm
from mtct_dft_mslh m1
where to_char(m1.tgl_dm,'yyyymmdd') >= to_char($P{tgl1}::date,'YYYYMMDD') 
and to_char(m1.tgl_dm,'yyyymmdd') <= to_char($P{tgl2}::date,'YYYYMMDD')
and m1.kd_plant = $P{lok_pt}
and (m1.kd_line = $P{kd_line} or $P{kd_line} = '')
and (m1.kd_mesin = $P{kd_mesin} or $P{kd_mesin} = '')
and exists (select 1 from mtcm_npk where mtcm_npk.npk = $P{npk} and mtcm_npk.kd_plant = m1.kd_plant)
order by m1.tgl_dm, m1.kd_mesin, m1.kd_line]]>
