	<queryString>
		<![CDATA[SELECT T1.SHIFT, T1.KD_MESIN AS MESIN, FNM_LINEX(T1.KD_LINE) LINE, 
T1.URAIAN_PROB, T1.URAIAN_PENYEBAB, T1.LANGKAH_KERJA, T1.CATATAN, 
TO_CHAR(T1.EST_JAMSTART,'HH24:MI') ESTJAMSTART, TO_CHAR(T1.EST_JAMEND,'HH24:MI') ESTJAMEND, 
COALESCE(T1.EST_DURASI,0) EST_DURASI, COALESCE(T1.LINE_STOP,0) LINE_STOP
FROM TMTCWO1 T1
WHERE TO_CHAR(T1.TGL_WO,'YYYYMMDD') = TO_CHAR($P{tgl1}::date,'YYYYMMDD')
AND T1.LOK_PT = $P{lok_pt}
AND (T1.SHIFT = $P{shift} OR $P{shift} = '')
AND (T1.KD_LINE = $P{kd_line} OR $P{kd_line} = '')
AND (T1.KD_MESIN = $P{kd_mesin} OR $P{kd_mesin} = '')
AND COALESCE(T1.INFO_KERJA,'-') = $P{info_kerja}
AND COALESCE(T1.LINE_STOP,0) >= COALESCE($P{line_stop},0)
AND EXISTS (SELECT 1 FROM MTCM_NPK WHERE MTCM_NPK.NPK = $P{npk} AND MTCM_NPK.KD_PLANT = T1.LOK_PT)
ORDER BY T1.EST_JAMSTART, T1.KD_MESIN]]>
	</queryString>
