	<queryString>
		<![CDATA[select kd_whfrom, kd_whto, kd_pono, to_date(to_char(tgl_trans,'dd-mm-yyyy'), 'dd-mm-yyyy') tgl_dn, no_pos, 
kd_item, (select baan_mpart.desc1||' ('||baan_mpart.itemdesc||')' from baan_mpart where baan_mpart.item = baan_iginh008s.kd_item) item_name, 
nm_trket, qty qty_dn, 
coalesce((select sum(qty_sj) from ppct_dnclaim_sj2s p where p.no_dn = baan_iginh008s.kd_pono and p.no_pos = baan_iginh008s.no_pos),0) qty_sj, 
coalesce((select sum(qty_sj) from ppct_dnclaim_sj2s p where p.no_dn = baan_iginh008s.kd_pono and p.no_pos = baan_iginh008s.no_pos),0) qty_act, 
kd_bpid1 as kd_bpid, (select b_suppliers.nama from b_suppliers where b_suppliers.kd_supp = baan_iginh008s.kd_bpid1) nm_bpid 
from baan_iginh008s 
where thn_kembali = $P{tahun} 
and bln_kembali = $P{bulan} 
and ('IGP'||substr(kd_whfrom,1,1) = $P{kd_site} or $P{kd_site} = '') 
and (kd_pono = $P{no_dn} or $P{no_dn} = '') 
and (st_tampil = $P{st_tampil} or $P{st_tampil} = '') 
and kd_bpid1 = $P{kd_bpid}
order by kd_pono, no_pos]]>
	</queryString>
