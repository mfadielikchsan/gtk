	<queryString>
		<![CDATA[select k1.status, (select d.desc_div from divisi d where d.kd_div = k1.kd_div limit 1) desc_div, k1.tahun, r.strategy_priority, 
r.strategy, r.kode, r.item, r.coy_kpi, r.target, r.initiatives, r.div, r.due_date, 
k2.kd_item, k2.kpi_div, k2.kpi_div_target, k2.activity, k2.kpi_ref, 
case when k1.status not in ('DRAFT', 'SUBMIT', 'REJECT', 'APPROVE HRD') then 'YES' else 'NO' end as st_q1, 
case when k1.status not in ('DRAFT', 'SUBMIT', 'REJECT', 'APPROVE HRD', 'APPROVE SUPERIOR', 'SUBMIT REVIEW Q1', 'REJECT REVIEW Q1') then 'YES' else 'NO' end as st_q2, 
case when k1.status not in ('DRAFT', 'SUBMIT', 'REJECT', 'APPROVE HRD', 'APPROVE SUPERIOR', 'SUBMIT REVIEW Q1', 'REJECT REVIEW Q1', 'APPROVE REVIEW Q1', 'SUBMIT REVIEW Q2', 'REJECT REVIEW Q2') then 'YES' else 'NO' end as st_q3, 
case when k1.status not in ('DRAFT', 'SUBMIT', 'REJECT', 'APPROVE HRD', 'APPROVE SUPERIOR', 'SUBMIT REVIEW Q1', 'REJECT REVIEW Q1', 'APPROVE REVIEW Q1', 'SUBMIT REVIEW Q2', 'REJECT REVIEW Q2', 'APPROVE REVIEW Q2', 'SUBMIT REVIEW Q3', 'REJECT REVIEW Q3') then 'YES' else 'NO' end as st_q4,  
COALESCE(k2.target_q1_4, COALESCE(k2.target_q1_3, COALESCE(k2.target_q1_2, k2.target_q1))) AS target_q1,
COALESCE(k2.target_q2_4, COALESCE(k2.target_q2_3, k2.target_q2)) AS target_q2,
COALESCE(k2.target_q3_4, k2.target_q3) AS target_q3,
k2.target_q4 AS target_q4, 
COALESCE(k2.tgl_start_q1_4, COALESCE(k2.tgl_start_q1_3, COALESCE(k2.tgl_start_q1_2, k2.tgl_start_q1))) AS tgl_start_q1,
COALESCE(k2.tgl_start_q2_4, COALESCE(k2.tgl_start_q2_3, k2.tgl_start_q2)) AS tgl_start_q2,
COALESCE(k2.tgl_start_q3_4, k2.tgl_start_q3) AS tgl_start_q3,
k2.tgl_start_q4 AS tgl_start_q4, 
COALESCE(k2.tgl_finish_q1_4, COALESCE(k2.tgl_finish_q1_3, COALESCE(k2.tgl_finish_q1_2, k2.tgl_finish_q1))) AS tgl_finish_q1,
COALESCE(k2.tgl_finish_q2_4, COALESCE(k2.tgl_finish_q2_3, k2.tgl_finish_q2)) AS tgl_finish_q2,
COALESCE(k2.tgl_finish_q3_4, k2.tgl_finish_q3) AS tgl_finish_q3,
k2.tgl_finish_q4 AS tgl_finish_q4, 
COALESCE(k2.target_q1_4_act, COALESCE(k2.target_q1_3_act, COALESCE(k2.target_q1_2_act, k2.target_q1_act))) AS target_q1_act,
COALESCE(k2.target_q2_4_act, COALESCE(k2.target_q2_3_act, k2.target_q2_act)) AS target_q2_act,
COALESCE(k2.target_q3_4_act, k2.target_q3_act) AS target_q3_act,
k2.target_q4_act AS target_q4_act, 
COALESCE(k2.tgl_start_q1_4_act, COALESCE(k2.tgl_start_q1_3_act, COALESCE(k2.tgl_start_q1_2_act, k2.tgl_start_q1_act))) AS tgl_start_q1_act,
COALESCE(k2.tgl_start_q2_4_act, COALESCE(k2.tgl_start_q2_3_act, k2.tgl_start_q2_act)) AS tgl_start_q2_act,
COALESCE(k2.tgl_start_q3_4_act, k2.tgl_start_q3_act) AS tgl_start_q3_act,
k2.tgl_start_q4_act AS tgl_start_q4_act, 
COALESCE(k2.tgl_finish_q1_4_act, COALESCE(k2.tgl_finish_q1_3_act, COALESCE(k2.tgl_finish_q1_2_act, k2.tgl_finish_q1_act))) AS tgl_finish_q1_act,
COALESCE(k2.tgl_finish_q2_4_act, COALESCE(k2.tgl_finish_q2_3_act, k2.tgl_finish_q2_act)) AS tgl_finish_q2_act,
COALESCE(k2.tgl_finish_q3_4_act, k2.tgl_finish_q3_act) AS tgl_finish_q3_act,
k2.tgl_finish_q4_act AS tgl_finish_q4_act, 
COALESCE(k2.persen_q1_4, COALESCE(k2.persen_q1_3, COALESCE(k2.persen_q1_2, COALESCE(k2.persen_q1, 0)))) AS persen_q1,
COALESCE(k2.persen_q2_4, COALESCE(k2.persen_q2_3, COALESCE(k2.persen_q2, 0))) AS persen_q2,
COALESCE(k2.persen_q3_4, COALESCE(k2.persen_q3, 0)) AS persen_q3,
COALESCE(k2.persen_q4, 0) AS persen_q4, 
COALESCE(k2.problem_q1_4, COALESCE(k2.problem_q1_3, COALESCE(k2.problem_q1_2, k2.problem_q1))) AS problem_q1,
COALESCE(k2.problem_q2_4, COALESCE(k2.problem_q2_3, k2.problem_q2)) AS problem_q2,
COALESCE(k2.problem_q3_4, k2.problem_q3) AS problem_q3,
k2.problem_q4 AS problem_q4, 
coalesce(k2.weight,0) weight, k2.keterangan, 
(select string_agg((select d.desc_dep from departement d where d.kd_dep = h.kd_dep limit 1), ', ' order by kd_dep) 
from hrdt_kpi_deps h where h.hrdt_kpi_act_id = k2.id) departemens, 
$P{kuning} logo_kuning, $P{merah} logo_merah, $P{hijau} logo_hijau, $P{abu} logo_abu 
from hrdt_kpis k1, hrdt_kpi_acts k2, hrdm_kpi_refs r 
where k1.id = k2.hrdt_kpi_id 
and k2.kpi_ref = r.id 
and k1.id = $P{id}
order by (case when k2.kd_item = 'FP' then 1 when k2.kd_item = 'CS' then 2 when k2.kd_item = 'IP' then 3 when k2.kd_item = 'LG' then 4 when k2.kd_item = 'CR' then 5 else 6 end), k2.id]]>
	</queryString>
