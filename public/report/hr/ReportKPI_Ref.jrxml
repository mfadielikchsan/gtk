	<queryString>
		<![CDATA[select distinct id_ref, tahun, kd_pt, kode, item, strategy_priority, strategy, coy_kpi, target, initiatives, div, due_date, kd_item, kd_item_urut, kd_div, fnm_div(kd_div) nm_div, catatan_q1, catatan_q2, catatan_q3, catatan_q4 
from (
	select k2.kd_item, (case when k2.kd_item = 'FP' then 1 when k2.kd_item = 'CS' then 2 when k2.kd_item = 'IP' then 3 when k2.kd_item = 'LG' then 4 when k2.kd_item = 'CR' then 5 else 6 end) kd_item_urut, k2.id, r.id id_ref, r.tahun, r.kd_pt, r.kode, r.item, r.strategy_priority, r.strategy, r.coy_kpi, r.target, r.initiatives, r.div, r.due_date, k1.kd_div, coalesce(k1.catatan_q1,'-') catatan_q1, coalesce(k1.catatan_q2,'-') catatan_q2, coalesce(k1.catatan_q3,'-') catatan_q3, coalesce(k1.catatan_q4,'-') catatan_q4 
	from hrdt_kpis k1, hrdt_kpi_acts k2, hrdm_kpi_refs r 
	where k1.id = k2.hrdt_kpi_id 
	and k2.kpi_ref = r.id 
	and k1.id = $P{id}
	order by 2, k2.id
) as v 
order by kd_item_urut, id_ref]]>
