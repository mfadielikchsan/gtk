	<queryString>
		<![CDATA[select vmk.nama, vmk.desc_div nm_div, vmk.desc_dep nm_dep, 
(case 
	when vmk.kd_pt = 'IGP' then 'PT. INTI GANDA PERDANA' 
	when vmk.kd_pt = 'GKD' then 'PT. GEMALA KEMPA DAYA' 
	when vmk.kd_pt = 'WEP' then 'PT. WAHANA EKA PARAMITRA' 
	when vmk.kd_pt = 'AND' then 'PT. ANDIN' 
	when vmk.kd_pt = 'AGI' then 'PT. ASANO GEAR INDONESIA'
	when vmk.kd_pt = 'AWI' then 'PT. AKASHI WAHANA INDONESIA'
	else '-'
end) nm_pt,
fnm_npk(npk_superior1) nm_superior1, fnm_npk(npk_superior2) nm_superior2, fnm_npk(npk_hrd) nm_hrd, hrdt_ipp1s.* 
from hrdt_ipp1s inner join v_mas_karyawan vmk on vmk.npk = hrdt_ipp1s.npk where hrdt_ipp1s.id = $P{id}]]>
	</queryString>
